        $(document).on("click", ".stayhome-video-element", function (event) {
      src = $(this).find('span.file').attr('data');
      if(src){
        $('#myVideosModal').find('video source').attr('src', src);
        $('#myVideosModal').find('video').attr('src', src);
        $('#myVideosModal').find('video')[0].load();
        // $('#myVideosModal').find('video')[0].play();
        $('#myVideosModal').modal();
      }
      console.log(src);
    });

    $('#myVideosModal').on('hidden.bs.modal', function () {
      $('#myVideosModal').find('video')[0].load();
      console.log("hidden.bs.modal");
    });

        jQuery.scrollDepth();
    });
